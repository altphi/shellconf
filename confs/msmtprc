defaults
tls on

account default
host smtp.gmail.com
port 587
from beck@j38.net
auth on
user beck@j38.net
passwordeval security find-internet-password -a beck@j38.net -s imap.gmail.com -w
tls_certcheck off

account opentable
host smtp.office365.com
port 587
from sbeck@opentable.com
auth on
user sbeck@opentable.com
passwordeval security find-internet-password -a sbeck@opentable.com -s outlook.office365.com -w
tls_certcheck off
